    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hot-loader": "^4.12.15",
    "react-router": "^3.0.5",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "superagent": "^3.5.2",

    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/utils/styleMock.js"
    }
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {

{
  "name": "comet",
  "version": "1.0.0",
  "description": "Editor for OpenAPI/Swagger",
  "precommit": [
    "lint:fix"
  ],
  "scripts": {
    "prelin": "echo Linting...",
    "lint": "npx lint",
    "prelint:fix": "echo Fix linting...",
    "lint:fix": "npx eslint --fix .",
	"prestart": "echo Starting dev server...",
    "start": "cross-env NODE_ENV=development && npx webpack-dev-server --config ./webpack/development.js --progress",
	"pretest":"echo Testing...",
    "test": "npx jest",
	"preremovedist":"echo Deleting dist folder...",
    "removedist": "rimraf dist",
	"prebuild":"echo Buidling...",
    "build": "npm run removedist && cross-env NODE_ENV=production && npx webpack --config ./webpack/production.js --progress --bail"
  },
  "engines": {
    "node": ">=12.13.1"
  },
  "jest": {
    "testRegex": ".*\\.(test|spec)\\.js$",
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/utils/styleMock.js"
    }
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "ajv": "^6.12.5",
    "ajv-errors": "^1.0.1",
    "ajv-keywords": "^3.5.2",
    "autoprefixer": "^9.6.4",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.3.1",
    "codemirror": "^5.61.0",
    "cross-env": "^7.0.3",
    "css-loader": "^3.2.0",
    "dompurify": "^2.2.8",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "js-yaml": "^4.1.0",
    "lodash.clonedeep": "^4.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mobx": "^5.14.0",
    "mobx-react": "^6.1.3",
    "nanoid": "^3.1.23",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "qs": "^6.10.1",
    "react": "^16.10.2",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^16.10.2",
    "react-draggable": "^4.4.3",
    "react-hot-loader": "^4.12.15",
    "react-tooltip": "^4.2.19",
    "rmraf": "^1.0.3",
    "sass-loader": "^8.0.0",
    "showdown": "^1.9.1",
    "style-loader": "^1.0.0",
    "tailwindcss": "^1.1.4",
    "terser-webpack-plugin": "^2.1.2",
    "uuid": "^8.3.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "yaml-js": "^0.2.3",
    "yaml-loader": "^0.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "enzyme": "^3.10.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "nock": "^11.4.0",
    "pre-commit": "^1.2.2",
    "sinon": "^7.5.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-dev-server": "^3.8.2"
  },
  "license": "MIT"
}

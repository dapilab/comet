{
  "name": "web",
  "version": "1.0.0",
  "description": "web project",
  "author": "wwayne",
  "precommit": [
    "lint"
  ],
  "scripts": {
    "lint": "make lint",
    "start": "make dev",
    "test": "make test",
    "prebuild": "rm -rf dist",
    "build": "webpack --config ./webpack/production.js --progress --bail"
  },
  "engines": {
    "node": ">=12.13.1"
  },
